{
  "extends": [
    "plugin:prettier/recommended",
    "prettier",
    "airbnb-base",
    "eslint:recommended"
  ],
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "printWidth": 200
      }
    ],
    "linebreak-style": 0,
    "no-param-reassign": "off",
    "no-shadow": "off",
    "prefer-destructuring": "off",
    "no-loop-func": "off"
  },
  "env" : {
    "es2021": true,
    "browser": true,
    "node": true
  }
}
